{
    "name": "fluwork/console",
    "description": "A powerful library to create console application in PHP.",
    "keywords": ["console", "cli", "terminal"],
    "homepage": "https://github.com/Fluwork/Console",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "YaFou",
            "homepage": "https://github.com/YaFou",
            "role": "Owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "dist": "*"
        }
    },
    "readme": ".github/README.md",
    "support": {
        "issues": "https://github.com/Fluwork/Console/issues",
        "source": "https://github.com/Fluwork/Console"
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "spatie/phpunit-watcher": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Fluwork\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluwork\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-next": "1.0.x-dev"
        }
    }
}
